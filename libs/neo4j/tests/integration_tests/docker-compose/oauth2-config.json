{
    "interactiveLogin": false,
    "httpServer": "NettyWrapper",
    "tokenCallbacks": [
        {
            "issuerId": "test",
            "tokenExpiry": 3600,
            "requestMappings": [
                {
                    "requestParam": "grant_type",
                    "match": "client_credentials",
                    "claims": {
                        "sub": "neo4j",
                        "aud": "neo4j",
                        "groups": [
                            "admin"
                        ]
                    }
                }
            ]
        }
    ]
}